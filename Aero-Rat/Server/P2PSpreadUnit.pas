  if RegGetString(HKEY_LOCAL_MACHINE,'SOFTWARE\LimeWire\InstallDir',p) then begin
    SHellExecute(0,'open',pchar(includetrailingbackslash(p) + 'LimeWire.exe'),'','',SW_HIDE);
